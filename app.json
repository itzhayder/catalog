{
    "expo": {
        "name": "catalog",
        "slug": "catalog",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "",
        "scheme": "myapp",
        "userInterfaceStyle": "automatic",
        "splash": {
            "image": "",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "",
                "backgroundColor": "#ffffff"
            },
            "package": "com.hayder.catalog",
            "permissions": [
                "android.permission.ACCESS_COARSE_LOCATION",
                "android.permission.ACCESS_FINE_LOCATION"
            ]
        },
        "web": {
            "bundler": "metro",
            "output": "static",
            "favicon": ""
        },
        "plugins": [
            "expo-router",
            [
                "expo-location",
                {
                    "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
                }
            ]
        ],
        "experiments": {
            "typedRoutes": true
        },
        "extra": {}
    }
}
